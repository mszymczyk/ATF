          editor="Sce.Atf.Controls.PropertyEditing.ColorPickerEditor,Atf.Gui.WinForms"
          converter="Sce.Atf.Controls.PropertyEditing.IntColorConverter" />
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="intervalType">
        <xs:sequence>
          <xs:element name="curve" type="curveType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:ID" use="required" />
        <xs:attribute name="length" type="xs:float" default="500.0"/>
        <xs:attribute name="color" type="xs:integer" default="-16776961"/>
        <!--blue-->
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>




  <xs:complexType name="trackFaderType">
