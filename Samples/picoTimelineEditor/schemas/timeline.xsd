        <xs:attribute name="length" type="xs:float" default="1000.0"/>
        <xs:attribute name="color" type="xs:integer" default="-8388608"/><!--dark green 0xff800000-->
        <!--blue-->
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  

  <xs:complexType name="intervalCameraAnimType">
