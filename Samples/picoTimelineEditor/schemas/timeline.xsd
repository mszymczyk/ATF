        <xs:attribute name="animOffset" type="xs:float" default="0.0"/>
        <xs:attribute name="color" type="xs:integer" default="-8388608"/>
        <xs:attribute name="animFile" type="xs:string" />

        <xs:attribute name="cameraView" type="xs:string" />
