  <xs:complexType name="trackAnimControllerType">
    <xs:annotation>
      <xs:appinfo>
        <scea.dom.editors menuText="TrackAnimController" description="TrackAnimController" image="picoTimelineEditor.Resources.track.png" category="pico" />
        <!--<scea.dom.editors.attribute name="name" displayName="Name" description="Unique Name" />-->
        <scea.dom.editors.attribute name="skelFile" displayName="Skel Filename" description="File name of animation skel"
                                    editor="pico.Controls.PropertyEditing.picoFileUriEditor,pico:Animation Skeleton files (*.skel)|*.skel"
                                    converter="Sce.Atf.Controls.PropertyEditing.ReadOnlyConverter"
                                    category="Animation" />
        <scea.dom.editors.attribute name="rootNode" displayName="Root node name" description="Node name to animate (containerName:nodeName)" category="Animation" />
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="trackType">
        <xs:sequence>
          <xs:element name="intervalAnimControllerType" type="intervalAnimControllerType" minOccurs="0" maxOccurs="1" />
        </xs:sequence>
        <!--<xs:attribute name="name" type="xs:ID" use="required" />-->
        <xs:attribute name="skelFile" type="xs:string" />
        <xs:attribute name="rootNode" type="xs:string" />
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  
  <xs:complexType name="intervalCurveType">
